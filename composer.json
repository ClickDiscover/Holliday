{
    "name": "clickdiscover/holliday",
    "type": "project",
    "homepage": "http://clickdiscover.io",
    "license": "proprietary",
    "require": {
        "php": ">=7.0",
        "roave/security-advisories": "dev-master",
        "zendframework/zend-expressive": "~1.0.0@rc || ^1.0",
        "zendframework/zend-expressive-helpers": "^2.0",
        "zendframework/zend-stdlib": "~2.7",
        "zendframework/zend-expressive-fastroute": "^1.0",
        "zendframework/zend-servicemanager": "^2.7.3 || ^3.0",
        "ocramius/proxy-manager": "^1.0",
        "zendframework/zend-expressive-twigrenderer": "^1.0",
        "robloach/component-installer" : "^0.2.3",
        "components/jquery" : "^2.2",
        "components/pure": "dev-master",
        "ramsey/uuid": "^3.1",
        "illuminate/contracts": "^5.2",
        "illuminate/support": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.3",
        "filp/whoops": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "ClickDiscover\\": "src/ClickDiscover/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/AppTest/"
        }
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "cs": "phpcs",
        "cs-fix": "phpcbf",
        "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php",
        "test": "phpunit"
    },
    "config": {
      "component-dir": "public/components"
    }
}
